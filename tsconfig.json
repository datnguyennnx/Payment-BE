{
  "compilerOptions": {
      "forceConsistentCasingInFileNames": true,
      "module": "commonjs",
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "strictNullChecks": false,
      "esModuleInterop": true,
      "target": "es6",
      "skipLibCheck": true,
      "strict": true
  },
  "include": ["src/**/*"],
  "exclude": [
    "node_modules"
   ]
}